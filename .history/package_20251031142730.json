{
  "name": "@geckoai/gecko-tsyringe",
  "version": "1.0.0",
  "description": "A powerful TypeScript dependency injection container extended from tsyringe, providing a modular dependency management solution.",
  "main": "dist/cjs",
  "types": "dist/types",
  "module": "dist/esm",
  "scripts": {
    "test": "mocha",
    "lint": "eslint src/ --fix",
    "build:clean": "rimraf dist && rimraf docs",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:amd": "tsc --project tsconfig.prod.amd.json",
    "build:esm": "tsc --project tsconfig.prod.esm.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "build:docs": "typedoc --options ./typedoc.json",
    "release": "pnpm run build:cjs && pnpm run build:amd && pnpm run build:esm && pnpm run build:types "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geckoai/gecko-tsyringe.git"
  },
  "dependencies": { 
    "reflect-metadata": "^0.1.12",
    "@geckoai/class-mirror": "^1.0.2"
  },
  "devDependencies": {
    "typescript": "4.9.5",
    "typedoc": "0.26.11",                          
    "ts-node": "^10.9.2",
    "mocha": "^11.7.1",
    "@types/mocha": "^10.0.10",
    "@types/chai": "^4.2.22",
    "chai": "^4.3.4",
    "eslint": "9.24.0",
    "@eslint/js": "9.24.0",
    "@eslint/json": "0.11.0",
    "typescript-eslint": "8.29.1",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0"
  },
  "peerDependencies": {
    "typescript": "^4"
  },
  "license": "MIT"
}
